{
  "name": "drop-compress-image",
  "version": "2.0.0",
  "description": "DropImageCompress is a desktop application that converts images to WebP/Avif format using drag-and-drop functionality. Built with Vite, Vue 3, TypeScript, and Tauri.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/DropWebP",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/DropWebP.git"
  },
  "bugs": {
    "url": "https://github.com/logue/DropWebP/issues"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0",
    "pnpm": ">=10.2.0"
  },
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "dev": "vite",
    "dev:tauri": "tauri dev",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" build:tauri --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "build:tauri": "tauri build",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "tauri": "tauri",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource-variable/noto-sans-jp": "5.2.6",
    "@mdi/font": "7.4.47",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-fs": "^2.4.2",
    "@tauri-apps/plugin-http": "^2.5.2",
    "@tauri-apps/plugin-opener": "^2.5.0",
    "@vueuse/sound": "^2.1.3",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "unified-network": "0.6.4",
    "vue": "3.5.21",
    "vue-i18n": "^11.1.12",
    "vuetify": "3.9.7",
    "webfontloader": "^1.6.28",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^11.0.0",
    "@modyfi/vite-plugin-yaml": "^1.1.1",
    "@tauri-apps/cli": "2.8.4",
    "@tsconfig/node-lts": "^22.0.2",
    "@types/node": "^24.3.1",
    "@types/webfontloader": "^1.6.38",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.35.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-vuetify": "^2.5.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "npm-run-all2": "^8.0.4",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-embedded": "^1.92.1",
    "stylelint": "^16.23.1",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4",
    "vite-plugin-checker": "0.10.3",
    "vite-plugin-vue-devtools": "^8.0.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vue-tsc": "^3.0.6"
  }
